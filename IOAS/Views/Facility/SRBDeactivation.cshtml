@{
    ViewBag.Title = "SRB";
    ViewBag.Subtitle = "SRB Deactivation";
    Layout = "~/Views/Shared/_LayoutFluid.cshtml";
}
<script src="~/Scripts/Facility/SRBDeactivation.js"></script>
<div class="row hd-tt-bg">
    <div class="col-md-6">
        <h3>SRB Deactivation</h3>
    </div>
</div>
<div class="row mt30">
    <div class="page-wrapper">
        <div class="row">
            <div class="col-md-12">
                <div class="custom-jsgrid-normal">
                    <div id="SRBDeactivationList"></div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="popupBuyback"></div>
<script type="text/javascript">
    var SuccMsg = '@TempData["PostSuccMsg"]';
    var ErrMsg = '@TempData["PostErrMsg"]';
    $(document).ready(function () {
        if (SuccMsg) {
            $('#alertSuccess').html(SuccMsg);
            $('#Success').modal('toggle');
        }
        else if (ErrMsg) {
            $('#FailedAlert').html(ErrMsg);
            $('#Failed').modal('toggle');
        }
    });
</script>
