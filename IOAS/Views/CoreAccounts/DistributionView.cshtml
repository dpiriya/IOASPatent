                                                                @Html.ValidationMessage("DistributionDetails[0].Category")
                                                                @Html.TextBox("DistributionDetails[0].DistributionDetailId", "", new { @class = "form-control dis-none" })
                                                                @Html.Hidden("DistributionDetails.Index", 0)
