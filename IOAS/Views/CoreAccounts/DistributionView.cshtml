                                                                @Html.HiddenFor(m => m.InstituteOverheadPercentage)
                                                                @Html.HiddenFor(m => m.InstituteOverheadAmount)
                                                            </div>
                                                        </div>
                                                        <div class="row">
                                                            <div class="form-group custom-form-group mb-md">
                                                                <b>
                                                                    <label class="col-md-7" style="font-weight:bold">Amount Available for Staff:</label>
                                                                    <span class="col-md-3 mt-xs" id="amtavailforstaff">@Html.DisplayFor(m => m.AmountAvailableforDistribution)</span>
                                                                </b>
