                                                        @Html.HiddenFor(m => m.ProjectType)

                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group custom-form-group">
                                                        <label>Project No </label>
                                                        @Html.TextBoxFor(m => m.ProjectNumber, new { @class = "form-control", @id = "txtProjectNumber", @onchange = "calculateamount();" })
                                                        @Html.TextBoxFor(m => m.ProjectId, new { @class = "form-control dis-none" })
                                                        @Html.ValidationMessageFor(m => m.ProjectNumber)
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="vh-grid-tb mt-null mb-lg">
                                                <div class="vh-grid-cell grey-bg">
                                                    <div class="grey-bg-adj">
                                                        <div class="row">
                                                            <div class="col-md-6">
                                                                <div class="form-group custom-form-group">
                                                                    <label>PI Name :</label>
                                                                    <h4 id="piname">@Html.DisplayFor(m => m.PIName)</h4>
                                                                    @Html.HiddenFor(m => m.PIName)
                                                                    @Html.HiddenFor(m => m.PIId)
                                                                    @Html.HiddenFor(m => m.ProjectType)
                                                                </div>
                                                                <div class="form-group custom-form-group">
                                                                    <label>Total Spent Amount :</label>
                                                                    <h4 id="totamtspent">@Html.DisplayFor(m => m.PjctTotalAmountSpent)</h4>
                                                                </div>
                                                                <div class="form-group custom-form-group">
                                                                    <label>Open Commitment :</label>
                                                                    <h4 id="pjctopencommit">@Html.DisplayFor(m => m.PjctOpenCommitmentAmount)</h4>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6">
                                                                <div class="form-group custom-form-group">
                                                                    <label>Total Received :</label> <br />
                                                                    Receipt Amount : <span id="totamtreceivedinclOHandGST"> @Html.DisplayFor(m => m.PjctTotalAmtReceivedincldngOHandGST)</span> <br />
                                                                    Overheads Amount : <span id="totamtreceivedOH"> @Html.DisplayFor(m => m.PjctTotalAmountReceivedOH)</span><br />
                                                                    GST Amount : <span id="totamtreceivedGST"> @Html.DisplayFor(m => m.PjctTotalAmountReceivedGST)</span><br />
                                                                    <b> Balance Amount : <span id="totamtreceivedBalance"> @Html.DisplayFor(m => m.PjctTotalAmountReceived)</span><br /></b>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="row">

                                                        </div>

                                                    </div>

                                                </div>
                                            </div>



