                                                        <div class="form-group custom-form-group">
                                                            <label>Month & Year </label>
                                                            @Html.TextBoxFor(m => m.DistributionDate, new { @class = "form-control", @id = "txtDistributionDate" })
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group custom-form-group">
                                                            <label>Distribution Amount </label>
                                                            @Html.TextBoxFor(m => m.DistributionAmount, new { @class = "form-control", @id = "txtDistributionAmount", @onchange = "calculateamount();" })
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="lt-bl-bg">
