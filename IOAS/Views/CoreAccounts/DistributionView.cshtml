                                        <div class="col-md-12 scrl-table scrl-table-po-bl">
                                            <table class="table alter-table mb-null">
                                                <thead>
                                                    <tr>
                                                        <th style="width:150px;">Type</th>
                                                        <th style="width:270px;">Name</th>
                                                        <th style="width:200px;">Designation</th>
                                                        <th style="width:200px;">Department</th>
                                                        <th style="width:200px;">PayBill No	</th>
                                                        <th style="width:200px;">Project No</th>
                                                        <th style="width:150px;">Amount</th>
                                                        @*<th>TDS</th>
                                                            <th>Net</th>*@
                                                        <th style="width:150px;">Payment Mode</th>
                                                        @*<th>Bank Name</th>
                                                            <th>Branch </th>
                                                            <th>A/C No</th>
                                                            <th>IFSC</th>*@
                                                    </tr>
