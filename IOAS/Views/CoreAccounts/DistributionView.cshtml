                                                            <div class="col-md-6">
                                                                <div class="form-group custom-form-group mb-null">
                                                                    <label>Total Individual Share :</label>
                                                                    @Html.TextBoxFor(m => m.TotalIndividualShareAmount, new { @class = "form-control", @id = "txtTotalIndividualShare", @readonly = true })
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6">
                                                                <div class="form-group custom-form-group mb-null">
                                                                    <label>Total PCF :</label>
                                                                    @Html.TextBoxFor(m => m.TotalPCFShareAmount, new { @class = "form-control", @id = "txtTotalPCFShare", @readonly = true })
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="col-md-6">
                                            <div class="grey-bg-adj">
