                                                                <div class="col-md-12">
                                                                    <h4>Expenditure</h4>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <label class="col-md-6">Lab Code :</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.LabCode, new { @class = "form-control", @id = "txtToLabCode" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <label class="col-md-6">EUCO</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.EUCOAmount, new { @class = "form-control", @id = "txtEUCOAmount", @onchange = "calculateamount();", @onkeypress = "return ValidateDecimalOnly(event)", @autocomplete = "off" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <label class="col-md-6"> STORES CONSUMED</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.StoresConsumedAmount, new { @class = "form-control", @id = "txtStoresAmount", @onchange = "calculateamount();", @onkeypress = "return ValidateDecimalOnly(event)", @autocomplete = "off" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <label class="col-md-6">CENTRAL FACILITIES</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.CentralFacilitiesAmount, new { @class = "form-control", @id = "txtCentralFacilitiesAmount", @onchange = "calculateamount();", @onkeypress = "return ValidateDecimalOnly(event)", @autocomplete = "off" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <label class="col-md-6">OTHERS</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.OthersAmount, new { @class = "form-control", @id = "txtOthersAmount", @onchange = "calculateamount();", @onkeypress = "return ValidateDecimalOnly(event)", @autocomplete = "off" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group mb-null">
                                                                    <label class="col-md-6">Total</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.TotalExpenditureAmount, new { @class = "form-control", @id = "txtExpenditureAmount", @onkeypress = "return ValidateDecimalOnly(event)", @autocomplete = "off" })
                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="lt-bl-bg">
                                                            <div class="row">
                                                                <div class="col-md-12">
                                                                    <h4>Amount Transferred to IIT</h4>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <label class="col-md-6">Power/Water/Others</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.TransferedToIITAmount, new { @class = "form-control", @id = "txtTransferedToIITAmount", @onchange = "calculateamount();", @onkeypress = "return ValidateDecimalOnly(event)", @autocomplete = "off" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <label class="col-md-6"> Subtotal</label>
                                                                    <div class="col-md-6">
                                                                        @Html.TextBoxFor(m => m.SubTotalAmount, new { @class = "form-control", @id = "txtSubTotalAmount" })
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="row">
                                                                <div class="form-group custom-form-group">
                                                                    <b>
                                                                        <label class="col-md-7" style="font-weight:bold">Amount Available for Distribution:</label>
                                                                        <span class="col-md-3 mt-xs" id="amtavailfordistri"></span>
                                                                    </b>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="tab">
                <div class="">

                    <div class="col-md-12">
                        <div class="bl-lg-panel-box pb-null">
                            <div class="cmn-panel-box">
                                <div class="text-right mb20">
                                    <a href="javascript:void(0)" id="btnAddPayment" class="btn btn-primary">Add New</a>
                                </div>
                                <div class="grey-bg-adj mb-lg">
