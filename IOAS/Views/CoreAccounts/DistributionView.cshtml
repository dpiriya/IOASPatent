                                                        {
                                                            var category = "DistributionDetails[" + count + "].Category";
                                                            var userid = "DistributionDetails[" + count + "].UserId";
                                                            var name = "DistributionDetails[" + count + "].Name";
                                                            var distributiondetailsId = "DistributionDetails[" + count + "].DistributionDetailId";
                                                            var facultydept = "DistributionDetails[" + count + "].FacultyDepartment";
                                                            var facultydesig = "DistributionDetails[" + count + "].FacultyDesignation";
                                                            var paybillnumber = "DistributionDetails[" + count + "].PayBillNumber";
                                                            var projectnumber = "DistributionDetails[" + count + "].ProjectNumber";
                                                            var amount = "DistributionDetails[" + count + "].Amount";
                                                            //var tdsvalue = "DistributionDetails[" + count + "].TDSValue";
                                                            //var netamount = "DistributionDetails[" + count + "].NetAmount";
                                                            var paymentmode = "DistributionDetails[" + count + "].PaymentMode";
                                                            //var bankname = "DistributionDetails[" + count + "].BankName";
                                                            //var bankbranch = "DistributionDetails[" + count + "].BranchName";
                                                            //var accountnumber = "DistributionDetails[" + count + "].AccountNumber";
                                                            //var ifsccode = "DistributionDetails[" + count + "].IFSCCode";
                                                            var autoComp = "DistributionDetails[" + count + "].autoComplete";
                                                            count++;
                                                            <tr>

                                                                <td>
                                                                    @Html.DropDownList(@category, new SelectList(ViewBag.FacultyType, "id", "name", item.Category), "Select any", new { @class = "form-control required", @id = "txtfacultytype" })
                                                                    @Html.ValidationMessage(@category)
                                                                    @Html.TextBox(@distributiondetailsId, item.DistributionDetailId, new { @class = "form-control dis-none" })

                                                                </td>
                                                                <td class="tdDDLUser">
                                                                    @Html.TextBox(@autoComp, item.Name, new { @class = "form-control", @id = "txtname" })
                                                                    @Html.TextBox(@userid, item.UserId, new { @class = "form-control dis-none" })
                                                                    @Html.ValidationMessage(@userid)
                                                                    @Html.Hidden("DistributionDetails.Index", @count)
                                                                </td>
                                                                <td class="tdTxtName dis-none">
                                                                    @Html.TextBox(@name, item.Name, new { @class = "form-control" })
                                                                    @Html.ValidationMessage(@name)
                                                                </td>
                                                                <td>
                                                                    @Html.TextBox(@facultydesig, item.FacultyDesignation, new { @class = "form-control", @readonly = true })
                                                                    @Html.ValidationMessage(@facultydesig)
                                                                </td>
                                                                <td>
                                                                    @Html.TextBox(@facultydept, item.FacultyDepartment, new { @class = "form-control", @readonly = true })
                                                                    @Html.ValidationMessage(@facultydept)
                                                                </td>
                                                                <td>
                                                                    @Html.TextBox(@paybillnumber, item.PayBillNumber, new { @class = "form-control", @readonly = true })
                                                                    @Html.ValidationMessage(@paybillnumber)
                                                                </td>
                                                                <td>
                                                                    @Html.TextBox(@projectnumber, item.ProjectNumber, new { @class = "form-control", @readonly = true })
                                                                    @Html.ValidationMessage(@projectnumber)
                                                                </td>
                                                                <td>
                                                                    @Html.TextBox(@amount, item.Amount, new { @class = "form-control", @onchange = "calculatefacultyamount()", @onkeypress = "return ValidateDecimalOnly(event)" })
                                                                    @Html.ValidationMessage(@amount)
                                                                </td>
                                                                @*<td>
                                                                        @Html.TextBox(@tdsvalue, item.TDSValue, new { @class = "form-control", @onchange = "calculatetdsamount()", @onkeypress = "return ValidateDecimalOnly(event)" })
                                                                        @Html.ValidationMessage(@tdsvalue)
                                                                    </td>
                                                                    <td>
                                                                        @Html.TextBox(@netamount, item.NetAmount, new { @class = "form-control", @onchange = "calculatenetamount()", @readonly = true })
                                                                        @Html.ValidationMessage(@netamount)
                                                                    </td>*@
                                                                <td>
                                                                    @Html.DropDownList(@paymentmode, new SelectList(ViewBag.PaymentMode, "id", "name", item.PaymentMode), "Select any", new { @class = "form-control required", @id = "txtpaymentmode" })
                                                                    @Html.ValidationMessage(@paymentmode)
                                                                </td>

                                                                @*<td>
                                                                        @Html.TextBox(@bankname, item.BankName, new { @class = "form-control", @readonly = true })
                                                                        @Html.ValidationMessage(@bankname)

                                                                    </td>
                                                                    <td>
                                                                        @Html.TextBox(@bankbranch, item.BranchName, new { @class = "form-control", @readonly = true })
                                                                        @Html.ValidationMessage(@bankbranch)
                                                                    </td>
                                                                    <td>
                                                                        @Html.TextBox(@accountnumber, item.AccountNumber, new { @class = "form-control", @readonly = true })
                                                                        @Html.ValidationMessage(@accountnumber)
                                                                    </td>
                                                                    <td>
                                                                        @Html.TextBox(@ifsccode, item.IFSCCode, new { @class = "form-control", @readonly = true })
                                                                        @Html.ValidationMessage(@ifsccode)
                                                                    </td>*@
                                                                <td>
                                                                    <a href="javascript:void(0)" class="btn-circle dis-none removeDetail"><i class="ion-trash-a"></i></a>
                                                                </td>
                                                            </tr>

                                                        }
                                                    }
                                                    else
                                                    {
