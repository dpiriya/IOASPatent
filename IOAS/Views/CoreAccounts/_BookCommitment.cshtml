                                                                    </div>
                                                                    <div class="form-group custom-form-group" id="doclink">
                                                                        @Html.ActionLink(Model.DocName, "ShowDocument",
                                                            new { Controller = "Account", file = Model.AttachPath, filepath = "~/Content/CommitmentDocument/" },
